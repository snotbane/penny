[sub_resource type="GDScript" id="GDScript_365wd"]
script/source = "extends TypewriterTextEffect

const DEFAULT_SPEED := 10.0

var bbcode = \"dropin\"

func _process_custom_typewriter_fx(char_fx: CharFXTransform, tw: Typewriter, time_visible: float) -> bool:
	var alpha : float = time_visible * char_fx.env.get(&\"speed\", DEFAULT_SPEED)
	alpha = clampf(alpha, 0.0, 1.0)
	char_fx.color = lerp(
		Color(char_fx.color.r, char_fx.color.g, char_fx.color.b, 0.0),
		char_fx.color,
		alpha
	)
	var scale = 1.0
	char_fx.transform.origin.x = lerp(
		char_fx.transform.origin.x - char_fx.env.get(&\"x\", 0.0) / scale,
		char_fx.transform.origin.x,
		alpha
	)
	char_fx.transform.origin.y = lerp(
		char_fx.transform.origin.y - char_fx.env.get(&\"y\", 6.0) / scale,
		char_fx.transform.origin.y,
		alpha
	)

	return true
"

[resource]
script = SubResource("GDScript_365wd")
