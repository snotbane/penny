[sub_resource type="GDScript" id="GDScript_hjcn3"]
script/source = "extends Decor

func encounter_open(element: DecorElement) :
	if element.args.get(&\"silent\", args[&\"silent\"]):
		element.owner.is_talking = false
	var duration : float = element.args.get(&\"delay\", args[&\"delay\"])
	if element.args.get(&\"scaled\", args[&\"scaled\"]):
		duration /= Typewriter.user_setting_print_speed
	await element.owner.delay(duration, wait_state)
"

